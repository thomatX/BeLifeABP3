<Window x:Class="WPFPruebas.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFPruebas"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="patenteTxt" HorizontalAlignment="Left" Height="23" Margin="173,62,0,0" TextWrapping="Wrap" Text="Patente" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="idMarcaTxt" HorizontalAlignment="Left" Height="23" Margin="173,109,0,0" TextWrapping="Wrap" Text="ID Marca" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="idModeloTxt" HorizontalAlignment="Left" Height="23" Margin="173,160,0,0" TextWrapping="Wrap" Text="IdModelo" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="anhoTxt" HorizontalAlignment="Left" Height="23" Margin="173,210,0,0" TextWrapping="Wrap" Text="Año" VerticalAlignment="Top" Width="120"/>
        <Button Content="Agregar" HorizontalAlignment="Left" Margin="173,285,0,0" VerticalAlignment="Top" Width="120" Height="32" Click="Button_Click"/>

    </Grid>
</Window>
